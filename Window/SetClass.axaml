<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450" Width="300" Height="500"
        x:Class="vaYolo.Views.SetClass"
		HasSystemDecorations="False"
        Title="SetClass" FontSize="12">
	<ExperimentalAcrylicBorder CornerRadius="10" Grid.Row="0" Grid.Column="0">
		<ExperimentalAcrylicBorder.Material>
			<ExperimentalAcrylicMaterial TintColor="Black" TintOpacity="0.7"
							MaterialOpacity="0.7" BackgroundSource="Digger"/>
		</ExperimentalAcrylicBorder.Material>
		<ListBox Items="{Binding Items}" SelectionChanged="OnSelectionChanged"/>
	</ExperimentalAcrylicBorder>
</Window>
